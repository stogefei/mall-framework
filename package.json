{
  "name": "zaf",
  "version": "9.0.0",
  "description": "zaf-from",
  "private": true,
  "main": "index.js",
  "scripts": {
    "bootstrap": "npx lerna bootstrap",
    "changed": "lerna changed",
    "clean": "lerna clean",
    "task": "npx gulp --gulpfile ./build/gulp/index.js",
    "cz": "npx git-cz",
    "dev:portal": "yarn run build:packages && cd packages/portal && yarn run dev",
    "dev:admin": "cd packages/admin && yarn run dev",
    "build:api": "lerna run --scope=@amaz/api build",
    "build:theme": "lerna run --scope=@amaz/theme build",
    "build:packages": "yarn run build:theme && yarn run build:api"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stogefei/workspace-init.git"
  },
  "author": "zaf",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ],
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,vue,ts,tsx,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": "./plugins-config/cz-customizable-config-standard.js"
    }
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@types/dagre": "^0.7.47",
    "resize-observer-polyfill": "^1.5.1",
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@antv/x6-vue-shape": "^1.3.1",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/runtime-corejs3": "^7.13.10",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/acorn": "^4.0.6",
    "@types/fs-extra": "^9.0.11",
    "@vitejs/plugin-vue": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "acorn": "^8.7.1",
    "axios": "^0.26.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^8.0.3",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-insert": "^0.5.0",
    "gulp-replace": "^1.1.3",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^5.1.0",
    "husky": "^4.3.8",
    "less": "^3.9.0",
    "less-loader": "^8.0.0",
    "lint-staged": "^10.5.4",
    "minimist": "^1.2.5",
    "prettier": "^2.4.1",
    "progress": "^2.0.3",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.5",
    "vue-eslint-parser": "^8.0.0",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-typescript": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "^3.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "workspaces": [
    "packages/*"
  ]
}
